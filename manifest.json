{

  "manifest_version": 2,
  "name": "English-Czech Dictionary",
  "version": "1.1",

  "description": "An extension providing English to Czech translation",
  "homepage_url": "https://github.com/zedpowa/English-Czech-Dictionary#readme",
  "icons": {
    "48": "icons/dict-48.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"]
    }
  ],

  "permissions": [
    "*://glosbe.com/*"
  ],

  "web_accessible_resources": [
    "translator.html",
    "styles.css",
    "ua.svg",
    "da.svg",
    "dictionary.json",
    "./dictionary/1/*.json",
    "./dictionary/2/*.json",
    "./dictionary/3/*.json",
    "./dictionary/4/*.json"
  ]
}